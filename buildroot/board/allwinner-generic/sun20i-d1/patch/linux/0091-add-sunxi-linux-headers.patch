+	union {
+		struct {
+			uint8_t first;
+			uint8_t snd;
+			uint8_t rnd1;
+			uint8_t rnd2;
+		} val;
+		struct {
+			/*page read*/
+			uint8_t READ0;
+			uint8_t READSTART;
+			uint8_t RNOUT;
+			uint8_t RNOUTSTART;
+		} r;
+
+		struct {
+			/*page program/write*/
+			uint8_t SEQIN;
+			uint8_t PAGEPROG;
+			uint8_t RNDIN;
+		} w;
+
+		struct {
+			/*page cache write*/
+			uint8_t SEQIN;
+			uint8_t CACHEDPROG;
+			uint8_t RNDIN;
+		} cw;
