+#define NORMAL_REQ_GET_ADDR(__p_req, __p_addr_low, __p_addr_high)	\
+	int i = 0;							\
+									\
+	for (i = 0; i < __p_req->addr_cycles; i++) {			\
+		if (i < 4)						\
+			(*__p_addr_low) |= req->addr[i] << (i * 8);	\
+		else							\
+			(*__p_addr_high) |= req->addr[i] << ((i - 4) * 8);	\
+	}								\
