+		struct {
+			/*multi-plane page write*/
+			uint8_t SEQIN;
+			uint8_t MULTIPROG;
+			uint8_t RNDIN;
+		} mw;
+
+		struct {
+			/*page read*/
+			uint8_t READ0;
+			uint8_t MULTIREADSTART;
+			uint8_t RNOUT;
+			uint8_t RNOUTSTART;
+		} mr;
+
+	} cmd;
