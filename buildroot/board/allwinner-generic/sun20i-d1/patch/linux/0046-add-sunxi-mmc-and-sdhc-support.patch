+static int sunxi_mmc_mchk_r1_rdy(char *reg, int to_ns)
+{
+	int i = 0;
+	/*wait busy over*/
+	for (i = 0; i < to_ns; i++) {
+		if (!(mmc_mreadl(reg, REG_STAS) & SDXC_CARD_DATA_BUSY))
+			break;
+		ndelay(1);
+	}
+
+	if ((mmc_mreadl(reg, REG_STAS) & SDXC_CARD_DATA_BUSY)) {
+		printk("dead Wait r1 rdy failed\n");
+		return MWR_RFAIL;
+	}
+	return MWR_ROK;
+}
