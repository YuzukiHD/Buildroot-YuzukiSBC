+	dev_dbg(mmc_dev(smc_host->mmc), "sg len %d end des index %d\n",
+		data->sg_len, i - 1);
+
+	for_each_sg(data->sg, sg, data->sg_len, i) {
+		dev_dbg(mmc_dev(smc_host->mmc),
+			"sg %d, des[%d](%08x): [0] = %08x, [1] = %08x\n", i, i,
+			(u32) &pdes[i], (u32) ((u32 *) &pdes[i])[0],
