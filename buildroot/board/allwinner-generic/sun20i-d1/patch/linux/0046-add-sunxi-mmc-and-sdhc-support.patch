+struct sunxi_mmc_spec_regs {
+	u32 drv_dl;		/*REG_DRV_DL */
+	u32 samp_dl;		/*REG_SAMP_DL */
+	u32 ds_dl;		/*REG_DS_DL */
+	u32 sd_ntsr;//REG_SD_NTSR
+	u32 edsd;		/*REG_EDSD */
+	u32 csdc;		/*REG_CSDC */
+};
+
+enum sunxi_mmc_speed_mode {
+	SM0_DS26_SDR12 = 0,
+	SM1_HSSDR52_SDR25,
+	SM2_HSDDR52_DDR50,
+	SM3_HS200_SDR104,
+	SM4_HS400,
+	SM4_HS400_CMD,
+	SMX_UNUSED0,
+	SMX_UNUSED1,
+	SM_NUM,
+};
