+void sunxi_mmc_rescan_card(unsigned id)
+{
+
+	if (id > SUNXI_MAX_CONTROL) {
+		pr_err("%d id over max id", id);
+		return;
+	}
+
+	if (sunxi_hosts[id] == NULL) {
+		pr_err("sunxi_hosts[%d] should not be null", id);
