+#ifdef CONFIG_SUNXI_PANICPART
+static ssize_t sunxi_mmc_panic_read_ps(struct panic_part *part, loff_t sec_off,
+		size_t sec_cnt, char *buf)
+{
+	int ret;
+
+	ret = sunxi_mmc_panic_read(part->start_sect + sec_off, sec_cnt, buf);
