+		return ret;
+	return sec_cnt;
+}
+
+static ssize_t sunxi_mmc_panic_write_ps(struct panic_part *part, loff_t sec_off,
+		size_t sec_cnt, const char *buf)
+{
+	int ret;
+
+	ret =  sunxi_mmc_panic_write(part->start_sect + sec_off, sec_cnt, buf);
