+#define SDXC_DS_DL_SW_EN			(1u<<7)
+
+#define	SDXC_2X_TIMING_MODE			(1U<<31)
+#define SDXC_HS400_NEW_SAMPLE_EN			(1U<<0)
+
+/*mask*/
