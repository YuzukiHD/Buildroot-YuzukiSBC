+
+	if (aw_rawnand_detect_device(mtd)) {
+		if (aw_rawnand_device_match_idtab(mtd))
+			ret = 0;
+		else
+			awrawnand_err("dev can't found in idtab\n");
+	}
+
+	return ret;
