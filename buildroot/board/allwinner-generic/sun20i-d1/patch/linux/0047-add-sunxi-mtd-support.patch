+	ret = chip->block_bad(mtd, b);
+
+	chip->select_chip(mtd, -1);
+	mutex_unlock(&chip->lock);
+
+	awrawnand_mtd_trace("Exit %s ret@%d\n", __func__, ret);
+	return ret;
