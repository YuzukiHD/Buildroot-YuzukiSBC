+		ret = host->normal_op(chip, &req);
+		if (ret)
+			awrawnand_err("%s cmd@%d fail\n", __func__, RAWNAND_CMD_STATUS);
+	}
+
+out:
+	awrawnand_chip_trace("Exit %s status@%x\n", __func__, status);
+	return status;
+}
