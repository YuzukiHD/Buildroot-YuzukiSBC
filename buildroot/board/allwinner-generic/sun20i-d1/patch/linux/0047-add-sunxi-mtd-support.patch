+
+static inline bool aw_host_nfc_dma_int_occur_check(struct nfc_reg *nfc)
+{
+	return ((readl(nfc->sta) & NFC_DMA_INT_ENABLE) &&
+			(readl(nfc->int_ctl) & NFC_DMA_INT_ENABLE));
