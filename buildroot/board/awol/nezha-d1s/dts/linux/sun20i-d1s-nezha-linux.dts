	status = "disabled";
};

&sdc0 {
	bus-width = <4>;
	/*cd-gpios = <&pio PF 6 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>;*/
	/*non-removable;*/
	/*broken-cd;*/
