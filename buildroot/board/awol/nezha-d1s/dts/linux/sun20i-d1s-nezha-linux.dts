	lcd_hspw            = <0>;
	lcd_vbp             = <23>;
	lcd_vt              = <525>;
	lcd_vspw            = <0>;

	lcd_lvds_if         = <0>;
	lcd_lvds_colordepth = <1>;
	lcd_lvds_mode       = <0>;
	lcd_frm             = <1>;
	lcd_io_phase        = <0x0000>;
